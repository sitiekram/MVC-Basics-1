@model List<Person>
<div class="container" style="font-size: 20px; background-color: #96D4D4">
    <div class="row" style="font-weight:bold;font-size:25px">
        <div class="col">
            Person ID
        </div>
        <div class="col">
            Full Name
        </div>
        <div class="col">
            City
        </div>
        <div class="col">
            Phone Number
        </div>
        <div class="col">
            Email
        </div>
        <div class="col">
            Action
        </div>
    </div>
    @foreach (var person in Model)
    {
        <partial name="_PersonPartial" model="person" />
    }
</div>
